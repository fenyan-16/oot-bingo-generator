{
  "name": "bingo-generator-ts",
  "version": "1.0.0",
  "description": "Generator for OoT Bingo in TypeScript",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "ts-node src/sandbox/main.ts",
    "test": "jest",
    "bundle": "npx webpack",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xwmtp/oot-bingo-generator.git"
  },
  "keywords": [
    "oot",
    "bingo",
    "generator",
    "speedrun"
  ],
  "author": "",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/xwmtp/oot-bingo-generator/issues"
  },
  "homepage": "https://github.com/xwmtp/oot-bingo-generator#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^27.2.0",
    "eslint": "^8.15.0",
    "jest": "^27.2.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "printWidth": 90,
    "tabWidth": 2
  }
}
